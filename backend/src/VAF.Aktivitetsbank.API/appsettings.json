{
  "VafOptions": {
    "AdApi": "http://localhost:5000/",
    "AdApiPath": "api/",
    "TestInfo":  "Test" 
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=Aktivitetsbank_dev;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Information"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "System": "Verbose",
        "Microsoft": "Verbose"
      }
    },
    "Enrich": [
      "FromLogContext"
    ],
    "WriteTo": [
      { "Name": "DiagnosticTrace" },
      {
        "Name": "RollingFile",
        "Args": { "pathFormat": "c:\\tmp\\logs\\log-aktivitetsbank-dev-{Date}.txt" }
      }
    ]
  }
}