{
  "VafOptions": {
    "AdApi": "http://localhost:5000/",
    "AdApiPath": "api/"
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Information"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "System": "Verbose",
        "Microsoft": "Verbose"
      }
    },
    "Enrich": [
        "FromLogContext"
      ],
      "WriteTo": [
        {"Name":  "DiagnosticTrace"},
        {
          "Name": "RollingFile",
          "Args": { "pathFormat": "c:\\tmp\\logs\\log-aktivitetsbank-dev-{Date}.txt" }
        }
      ]
    }
  }